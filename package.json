{
  "name": "@aiready/monorepo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Explains why AI tools struggle with your codebase and where small changes unlock outsized AI leverage",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "dev": "turbo run dev --filter=!@aiready/platform --concurrency 11",
    "clean": "turbo run clean && rm -rf node_modules",
    "consume-reports": "node ./scripts/consume-aiready-reports.js",
    "graph": "nx graph",
    "visualize": "node scripts/visualize.js"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/d3": "^7.4.3",
    "@types/node": "^24.10.13",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "eslint": "^10.0.2",
    "npm-check-updates": "^19.3.2",
    "nx": "^22.5.1",
    "ovsx": "^0.10.9",
    "prettier": "^3.8.1",
    "semver": "^7.7.4",
    "turbo": "^2.8.9",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.30.3",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "d3": "^7.9.0"
  }
