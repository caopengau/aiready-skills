{
  "name": "@aiready/core",
  "version": "0.9.33",
  "description": "Shared utilities for AIReady analysis tools",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "require": "./dist/client.js",
      "import": "./dist/client.mjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/client.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/client.ts --format cjs,esm --watch",
    "lint": "eslint src",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build",
    "release": "pnpm build && pnpm publish --no-git-checks"
  },
  "keywords": [
    "aiready",
    "code-analysis",
    "utilities"
  ],
  "author": "AIReady Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/caopengau/aiready-core.git"
  },
  "homepage": "https://github.com/caopengau/aiready-core",
  "bugs": {
    "url": "https://github.com/caopengau/aiready-core/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CONTRIBUTING.md"
  ],
  "devDependencies": {
    "eslint": "^10.0.0",
    "tsup": "^8.3.5"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^8.53.0",
    "@typescript-eslint/typescript-estree": "^8.53.0",
    "chalk": "^5.4.1",
    "glob": "^13.0.0",
    "ignore": "^7.0.0",
    "typescript": "^5.9.3"
  }
}
